{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  // @template-customization-start
  "import": ["./dictionaries/cspell-starcraft2.json"],
  // @template-customization-end
  "files": ["**"],
  "enableFiletypes": ["*"],
  "enableGlobDot": true,
  "useGitignore": true,
  "ignorePaths": [
    "*.pyc",
    ".git/**",
    "dist/**",
    "node_modules/**",
    "package-lock.json",
    "pnpm-lock.yaml",
    "yarn.lock"
  ],
  "words": [
    "autocrlf",
    "bufferutil",
    "buttonname",
    "dbaeumer",
    "esbenp",
    "loglevel",
    "pbjs",
    "protoc",
    "sarisia",
    "technote",
    "typedoc"
  ]
}
